# Google Gemini API Key (Obligatory)
GEMINI_API_KEY=your_key_here

# Notion API Integration Token (Obligatory)
NOTION_API_KEY=secret_abc

# Confluence API Configuration (Optional)
CONFLUENCE_DOMAIN=yourcompany.atlassian.net
CONFLUENCE_USER_EMAIL=your-email@company.com
CONFLUENCE_API_TOKEN=your_confluence_api_token

# Gemini Model (default: gemini-2.0-flash-exp if not specified)
GEMINI_MODEL=gemini-2.0-flash-exp

# Server Port (Optional)
PORT=3001

# Node Environment (Optional)
NODE_ENV=development
